<?php

// Constant string syntaxes

declare(strict_types=1);

use Tester\Assert;

require __DIR__ . '/../bootstrap.php';

$test = <<<'XX'
	'',
	"",
	'Hi',
	"Hi",
	'!\'!\\!\a!',
	"!\"!\\!\$!\n!\r!\t!\f!\v!\e!\a",
	"!\xFF!\377!\400!\0!",
	XX;

$node = parseCode($test);

Assert::same(
	loadContent(__FILE__, __COMPILER_HALT_OFFSET__),
	exportNode($node),
);

__halt_compiler();
